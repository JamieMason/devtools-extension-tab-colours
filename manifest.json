{
  "name": "DevTools Tab Colours",
  "version": "0.4.0",
  "manifest_version": 2,
  "description": "Adds colours to the tabs in DevTools.",
  "devtools_page": "devtools.html",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
